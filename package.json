{
  "name": "pdf",
  "version": "0.0.0",
  "description": "Generate PDF files of articles",
  "main": "index.js",
  "engines": {
    "node": "14"
  },
  "scripts": {
    "start": "cross-env NODE_ENV=production node lib/index.js",
    "build": "babel src --out-dir lib",
    "heroku-postbuild": "npm run build",
    "precommit": "lint-staged",
    "dev": "rimraf lib && npm run build && (babel -w src --out-dir lib & nodemon -w lib -w .env lib/index.js)",
    "cloc": "find . -name '*.js' -not -path \"./node_modules*\" -not -path \"./lib*\" | xargs wc -l",
    "test": "node test/renderToFile.js"
  },
  "lint-staged": {
    "*.js": [
      "standard --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/orbiting/pdf.git"
  },
  "author": "",
  "license": "BSD-3-Clause",
  "bugs": {
    "url": "https://github.com/orbiting/pdf/issues"
  },
  "homepage": "https://github.com/orbiting/pdf#readme",
  "dependencies": {
    "@react-pdf/renderer": "^2.0.12",
    "apollo-fetch": "^0.7.0",
    "babel-preset-stage-0": "^6.24.1",
    "d3-time-format": "^2.1.1",
    "express": "^4.16.2",
    "graphql-tag": "^2.6.0",
    "hyphen": "^1.6.2",
    "mdast-react-render": "^1.2.0",
    "react": "^17.0.2",
    "react-apollo": "^2.0.4",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.2",
    "regiment": "https://github.com/orbiting/regiment"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^10.0.2",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "cross-env": "^7.0.2",
    "d3-queue": "^3.0.7",
    "dotenv": "^9.0.1",
    "eslint": "^7.13.0",
    "eslint-config-standard": "^16.0.1",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.6.1",
    "eslint-plugin-standard": "^4.0.0",
    "husky": "^3.0.0",
    "lint-staged": "^9.2.0",
    "node-fetch": "^2.1.2",
    "nodemon": "^2.0.6",
    "rimraf": "^3.0.2",
    "rw": "^1.3.3",
    "standard": "^16.0.2"
  }
}
